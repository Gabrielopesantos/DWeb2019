<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT ARQSITS (ARQELEM)* >
<!ELEMENT ARQELEM (TIPO | IDENTI | IMAGEM | DESCRI | CRONO | LUGAR | CODADM | FREGUE | CONCEL | CODAM | LATITU | LONGIT | ALTITU | ACESSO | QUADRO | TRAARQ | INTERP | INTERE | DESARQ | DEPOSI | BIBLIO | AUTOR | DATA)* >
<!ELEMENT TIPO (#PCDATA) >
